<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0099cc"
    tools:context="p2.b230.caffeinetrackerp2.MainLoggingActivity">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <ImageButton
            android:id="@+id/change_activity_left"
            android:onClick="changeActivityLeft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_media_rew"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="25dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/change_activity_right"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/change_activity_right"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/change_activity_right"
            android:onClick="changeActivityRight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_media_ff"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/logging_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/title_activity_MainLogging"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/change_activity_right"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/change_activity_right"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.423" />

        <Spinner
            android:id="@+id/caffeine_source_choose"
            android:layout_width="66dp"
            android:layout_height="83dp"
            android:background="@android:color/holo_orange_dark"
            android:layout_marginEnd="95dp"
            android:layout_marginRight="95dp"
            app:layout_constraintRight_toLeftOf="@+id/imageButton"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageButton2"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_editor_absoluteX="115dp" />

        <Spinner
            android:id="@+id/caffeine_amount_choose"
            android:layout_width="68dp"
            android:layout_height="83dp"
            android:layout_marginEnd="116dp"
            android:background="@android:color/holo_red_dark"
            android:layout_marginRight="116dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/caffeine_source_choose"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="95dp"
            android:layout_height="87dp"
            app:srcCompat="@android:drawable/arrow_up_float"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="12dp"
            android:layout_marginLeft="11dp"
            android:layout_marginRight="12dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="@+id/imageButton2"
            app:layout_constraintLeft_toRightOf="@+id/caffeine_amount_choose"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="0dp"
            android:layout_height="88dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="10dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="10dp"
            app:srcCompat="@android:drawable/arrow_down_float"
            tools:layout_editor_absoluteY="463dp"
            app:layout_constraintRight_toLeftOf="@+id/caffeine_source_choose"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline5"
            app:layout_constraintGuide_begin="20dp"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="20dp" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>

